extends ../layouts/basic

block title
    | My Sample App

block content
    aa-layout(row-height="150" columns="12")
        div.grid-element(data-x="0" data-y="0" data-width="3" data-height="5")
            img(src="http://lorempixel.com/500/1500/abstract/1")
        div.grid-element(data-x="3" data-y="0" data-width="3" data-height="2")
            img(src="http://lorempixel.com/500/1000/abstract/2")
        div.grid-element(data-x="6" data-y="0" data-width="2" data-height="1")
            img(src="http://lorempixel.com/500/1000/abstract/3")
        div.grid-element(data-x="8" data-y="0" data-width="4" data-height="4")
            img(src="http://lorempixel.com/500/1000/abstract/4")
        div.grid-element(data-x="0" data-y="5" data-width="3" data-height="1")
            img(src="http://lorempixel.com/500/1000/abstract/5")
        div.grid-element(data-x="3" data-y="4" data-width="6" data-height="2")
            img(src="http://lorempixel.com/1000/500/abstract/6")
        div.grid-element(data-x="9" data-y="5" data-width="1" data-height="1")
            img.portrait(src="http://lorempixel.com/500/500/abstract/7")
        div.grid-element(data-x="10" data-y="5" data-width="1" data-height="1")
            img.portrait(src="http://lorempixel.com/500/500/abstract/8")
        div.grid-element(data-x="11" data-y="5" data-width="1" data-height="1")
            img.portrait(src="http://lorempixel.com/500/500/abstract/9")

